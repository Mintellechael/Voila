<%- include('partials/header'); -%>





    <div class= "reveal-grid">

   <div class="before-after-section">

    <p class="reveal-name">
    Name : <%= firstName + " " + lastName %> <br />
    </p>

      <p class= "before-info">

        Before Weight :<%= beforeWeight %> lbs <br>
        Height : <%= height %>
        Before BMI: <%= beforeBmi %>
      </p>

      <p class= "after-info">
        After Weight :<%= afterWeight %> lbs <br>
        Height : <%= height %>
        Before BMI: <%= afterBmi %>

      </p>

      <p class="secret">

        The magician's secret : <%=secrets %>

      </p>

       </div>




      <div class="container-fluid b1">
          <img class="bf front" src= "<%= beforeFront %>" alt="Before Front Picture" />
          <img class="after-front" src= "<%= afterFront %>"  alt="After Front Picture" />
      </div>

      <div class="container-fluid b1 b-l-side second">
          <img class="bf l-side" src="<%= beforeLeft %>" alt="Before Side Picture" />
          <img class="after-side" src="<%= afterLeft %>" alt="After Side Picture" />
      </div>

      <div class="container-fluid b1 b-r-side third first">
          <img class="bf r-side" src="<%= beforeRight %>" alt="Before Side 2 Picture" />
          <img class="after-side2" src="<%= afterRight %>" alt="After Side 2 Picture" />
      </div>

      <div class="container-fluid b1 b-back fourth second">
        <img class="bf back" src="<%= beforeBack %>" alt="Before Back Picture" />
        <img class="after-back" src="<%= afterBack %>" alt="After Back Picture" />
      </div>

    </div>

<script type=" text/javascript" src="/javascripts/voila.js"> </script>

<%- include('partials/footer'); -%>
